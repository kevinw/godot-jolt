[godot-jolt]
version = "${PROJECT_VERSION_WITH_STATUS}"
build = "${PROJECT_VERSION_BUILD}"

[configuration]

entry_symbol = "${GDJ_ENTRY_POINT}"
compatibility_minimum = "${GDJ_GODOT_VERSION_MAJOR}.${GDJ_GODOT_VERSION_MINOR}"
compatibility_maximum = "${GDJ_GODOT_VERSION_MAJOR}.${GDJ_GODOT_VERSION_MINOR}"

[libraries]

windows.release.${GDJ_PRECISION}.x86_64 = "windows/godot-jolt_windows-x64.dll"
windows.debug.${GDJ_PRECISION}.x86_64 = "windows/godot-jolt_windows-x64_editor.dll"

windows.release.${GDJ_PRECISION}.x86_32 = "windows/godot-jolt_windows-x86.dll"
windows.debug.${GDJ_PRECISION}.x86_32 = "windows/godot-jolt_windows-x86_editor.dll"

linux.release.${GDJ_PRECISION}.x86_64 = "linux/godot-jolt_linux-x64.so"
linux.debug.${GDJ_PRECISION}.x86_64 = "linux/godot-jolt_linux-x64_editor.so"

linux.release.${GDJ_PRECISION}.x86_32 = "linux/godot-jolt_linux-x86.so"
linux.debug.${GDJ_PRECISION}.x86_32 = "linux/godot-jolt_linux-x86_editor.so"

macos.release.${GDJ_PRECISION} = "macos/godot-jolt_macos.framework"
macos.debug.${GDJ_PRECISION} = "macos/godot-jolt_macos_editor.framework"

ios.release.${GDJ_PRECISION} = "visionos/godot-jolt_visionos.framework"
ios.release.${GDJ_PRECISION}.simulator = "visionos/godot-jolt_visionos_simulator.framework"
ios.debug.${GDJ_PRECISION} = "visionos/godot-jolt_visionos.framework"
ios.debug.${GDJ_PRECISION}.simulator = "visionos/godot-jolt_visionos_editor_simulator.framework"

android.release.${GDJ_PRECISION}.arm64 = "android/libgodot-jolt_android-arm64.so"
android.debug.${GDJ_PRECISION}.arm64 = "android/libgodot-jolt_android-arm64_editor.so"

android.release.${GDJ_PRECISION}.arm32 = "android/libgodot-jolt_android-arm32.so"
android.debug.${GDJ_PRECISION}.arm32 = "android/libgodot-jolt_android-arm32_editor.so"

android.release.${GDJ_PRECISION}.x86_64 = "android/libgodot-jolt_android-x64.so"
android.debug.${GDJ_PRECISION}.x86_64 = "android/libgodot-jolt_android-x64_editor.so"

android.release.${GDJ_PRECISION}.x86_32 = "android/libgodot-jolt_android-x86.so"
android.debug.${GDJ_PRECISION}.x86_32 = "android/libgodot-jolt_android-x86_editor.so"
